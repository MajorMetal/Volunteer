<h1>Upcoming Events in Orange County</h1>
<p>
	<%= link_to "Add New Event", new_event_path %>
</p>

<% @events.each do |event| %>
	
	<h3>
		<%= link_to event.name, event %>
	</h3>
	<p><%= l event.when, format: :long %></p>
	<p><%= event.location %></p><br/>

	<p><%= image_tag event.image %>
	

	<p>
	<hr/>

<% end %>